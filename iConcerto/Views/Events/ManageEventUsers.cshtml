@model IEnumerable<iConcerto.Models.UserData>

<h3>Manage assigned users</h3>

<table class="table">
    <tr>
        <th>
            <label>User's email</label>
        </th> 
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.FirstMidName)
        </td>

        <td>
            <a href="@Url.Action("UnassignUserByAdmin",new {id = ViewBag.eventId, userId = item.UserDataId})" class="btn btn-danger"><i class="fas fa-user-minus"></i> Retreat</a>        </td>
    </tr>
}

</table>
